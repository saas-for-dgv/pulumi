{
  "name": "infra-with-pulumi",
  "version": "1.0.0",
  "description": "For developing and maintaining infra with pulumi.",
  "main": "src/index.ts",
  "scripts": {
    "test2": "jest",
    "test": "mocha --require ts-node/register '**/*.test.ts'",
    "test:watch": "mocha --require ts-node/register -- --watch --no-cache '**/*.test.ts'",
    "tree": "tree -I 'node_modules'",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "tdd": "npm test -- --watch",
    "tdd2": "jest --watch --testMatch='**/*.test.ts'"
  },
  "author": "Koustubh Desai",
  "license": "ISC",
  "dependencies": {
    "@pulumi/aws": "^6.34.0",
    "@pulumi/pulumi": "^3.115.2",
    "@pulumi/random": "^4.16.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "mocha": "^10.4.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
